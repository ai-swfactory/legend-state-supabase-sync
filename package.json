{
  "name": "@ai-swfactory/legend-state-supabase-sync",
  "version": "1.0.0",
  "description": "Legend State Supabase Sync utilities - enhanced sync primitives for @legendapp/state with Supabase",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ai-swfactory/legend-state-supabase-sync.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "access": "restricted"
  },
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "build": "panoptic-build",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "validate": "pnpm run type-check && pnpm run lint && pnpm run format:check && pnpm run test"
  },
  "dependencies": {
    "@legendapp/state": "3.0.0-beta.34",
    "@supabase/postgrest-js": "^1.19.4",
    "@supabase/supabase-js": "^2.47.11"
  },
  "devDependencies": {
    "@ai-swfactory/panoptic-presets": "^12.0.0",
    "@ai-swfactory/rollup-plugins": "^2.2.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^22.15.29",
    "@vitest/coverage-v8": "^3.1.4",
    "eslint": "^9.28.0",
    "jiti": "^2.4.2",
    "prettier": "^3.5.3",
    "semantic-release": "^25.0.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "peerDependencies": {
    "@legendapp/state": "3.0.0-beta.34",
    "@supabase/postgrest-js": "^1.19.4",
    "@supabase/supabase-js": "^2.47.11"
  }
}
